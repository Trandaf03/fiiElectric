<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbarStyle="insideInset"
    tools:context=".masini.pagina_masini"
    tools:ignore="SpeakableTextPresentCheck">

    <Button
        android:id="@+id/buton_masini_rabla"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Informații program rabla"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buton_masini_electricVScombustibil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Avantajele și dezavantajele mașinii electrice"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buton_masini_rabla" />

    <Button
        android:id="@+id/buton_masini_returnRatioMasini"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Când iși scoate mașina mea banii?"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buton_masini_electricVScombustibil" />


</androidx.constraintlayout.widget.ConstraintLayout>